</head>
<body>
	<nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
		<div class="container">
			<a class="navbar-brand" href="/">DocLoc</a>
			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
			</button>
			<div class="collapse navbar-collapse" id="navbarResponsive">
			<ul class="navbar-nav ml-auto">
				<li class="nav-item active">
				<a class="nav-link" href="/">Home</a>
				</li>
			</ul>
			</div>
		</div>
	</nav>
	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100 p-l-85 p-r-85 p-t-55 p-b-55">
				<form action="/doctors/register" class="login100-form validate-form flex-sb flex-w" method="POST">
					<span class="login100-form-title p-b-32">
						Doctor Registration
					</span>

					<span class="txt1 p-b-11">
						Name
					</span>
					<div class="wrap-input100 validate-input m-b-36" data-validate = "Name is required">
						<input class="input100" type="text" name="name" >
						<span class="focus-input100"></span>
					</div>

					<span class="txt1 p-b-11">
						Email
					</span>
					<div class="wrap-input100 validate-input m-b-36" data-validate = "Email is required">
						<input class="input100" type="text" name="email" >
						<span class="focus-input100"></span>
					</div>
					
					<span class="txt1 p-b-11">
						Password
					</span>
					<div class="wrap-input100 validate-input m-b-12" data-validate = "Password is required">
						<span class="btn-show-pass">
							<i class="fa fa-eye"></i>
						</span>
						<input class="input100" type="password" name="pass" >
						<span class="focus-input100"></span>
					</div>

					<span class="txt1 p-b-11">
						Address
					</span>
					<div class="wrap-input100 validate-input m-b-36" data-validate = "Address is required">
						<input class="input100" type="text" name="address" >
						<span class="focus-input100"></span>
					</div>
					
					<span class="txt1 p-b-11">
						Tariff
					</span>
					<div class="wrap-input100 validate-input m-b-36" data-validate = "Tariff is required">
						<input class="input100" type="text" name="tarif" >
						<span class="focus-input100"></span>
					</div>

					<span class="txt1 p-b-11">
						Working Hours
					</span>
					<div class="wrap-input100 validate-input m-b-36" data-validate = "Working Hours is required">
						<input class="input100" type="text" name="workingHours" >
						<span class="focus-input100"></span>
					</div>

					<div class="row">
						<div>
							<div class="col-4">
								<span class="txt1 p-b-11">
									Specialization
								</span>
							</div>
							<div class="col-8">
								<select name="SpecialisasiId">
									<option value=1> General Practicioner </option>
									<option value=2> Dental Care </option>
									<option value=3> Dermatology </option>
									<option value=4> Obstetrics and Gynecology </option>
									<option value=5> Pediatrics </option>
									<option value=6> Opthalmology </option>
								</select>
							</div>
						</div>
						<div>
							<div class="col-4">
								<span class="txt1 p-b-11">
									Location
								</span>
							</div>
							<div class="col-8">
								<select name="LocationId">
									<option value=1> South Jakarta </option>
									<option value=2> North Jakarta </option>
									<option value=3> Central Jakarta </option>
									<option value=4> West Jakarta </option>
									<option value=5> East Jakarta </option>
								</select>
							</div>
						</div>
					</div>
					<br>
					<div class="container-login100-form-btn mt-5">
						<button class="login100-form-btn" type="submit">
							Register
						</button>
					</div>

				</form>
			</div>
		</div>
	</div>
	

	<div id="dropDownSelect1"></div>
	
